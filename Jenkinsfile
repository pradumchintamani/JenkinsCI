pipeline{
    agent any
    stages{
        stage('CheckOut') 
        steps{
            echo "This is Checkout stage for sample pipeline"
        }
        stage('Build')
        steps {
            echo "This is Build stage for sample pipeline"
        }
        stage('Test') 
        steps{
            echo "This is Test stage for sample pipeline"
        }
        stage('Deploy') 
        steps{
            echo " This is Deploy stage for sample pipeline"
        }
    }
}
